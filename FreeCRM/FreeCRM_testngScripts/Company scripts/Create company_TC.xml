<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FreeCRM Testing">
	<parameter name="baseURL"
		value="https://www.freecrm.com/index.html" />
	<parameter name="Browser" value="chrome" />
	<test name="Companies testing">
		<classes>
			<class name="registerationANDLogin.Login">
				<methods>
					<include name="loginUser" />
				</methods>
			</class>
			<class name="manageCompanies.Company">
				<methods>
					<include name="addCompany" />
				</methods>
			</class>
		</classes>
	</test>
</suite>